{
  "manifest_version": 3,
  "name": "To-Do List",
  "description": "A simple to-do list Chrome extension.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "48": "images/icon48.png"
  }
}
